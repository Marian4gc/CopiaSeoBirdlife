{% extends 'base.html.twig' %}

{% block title %}Totaldata index{% endblock %}

{% block body %}
    <h1>Totaldata index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for totaldatum in totaldatas %}
            <tr>
                <td>{{ totaldatum.id }}</td>
                <td>
                    <a href="{{ path('app_totaldata_show', {'id': totaldatum.id}) }}">show</a>
                    <a href="{{ path('app_totaldata_edit', {'id': totaldatum.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_totaldata_new') }}">Create new</a>
{% endblock %}
